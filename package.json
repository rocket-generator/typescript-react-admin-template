{
  "name": "toraq-dashboard-v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "*",
    "npm": "*",
    "yarn": "0"
  },
  "scripts": {
    "build": "vite build",
    "eslint-print-config": "eslint -c .eslintrc.cjs --print-config src/main.ts > misc/eslint-print-config.json",
    "lint-dry:eslint": "eslint --cache --cache-location node_modules/.cache/eslint/ --cache-strategy content --max-warnings=0 .",
    "lint-dry:format": "prettier --cache --check .",
    "lint-dry:tsc": "tsc --noEmit",
    "lint-dry": "run-s --continue-on-error 'lint-dry:*'",
    "lint:eslint": "eslint --max-warnings=0 --fix .",
    "lint:format": "prettier --cache --loglevel warn --write .",
    "lint": "run-s --continue-on-error 'lint:*' lint-dry:tsc",
    "preview": "vite preview",
    "start": "vite",
    "test-ci": "vitest --run --coverage",
    "test": "vitest",
    "update-all-deps": "npx npm-check-updates@16.10.12 --configFileName .ncurc.jsonc"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@mantine/core": "^5.10.4",
    "@mantine/form": "^5.10.4",
    "@mantine/hooks": "^5.10.4",
    "@mantine/notifications": "^5.10.4",
    "@refinedev/cli": "2.7.2",
    "@refinedev/core": "4.28.0",
    "@refinedev/kbar": "1.1.2",
    "@refinedev/mantine": "2.24.0",
    "@refinedev/react-router-v6": "4.3.2",
    "@refinedev/react-table": "5.6.0",
    "@refinedev/simple-rest": "4.5.0",
    "@tabler/icons": "^1.1.0",
    "@vitejs/plugin-react": "4.0.3",
    "axios": "^0.26.1",
    "date-fns": "2.30.0",
    "fp-ts": "2.16.1",
    "jose": "4.14.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.8.1",
    "react-use": "17.4.0",
    "utility-types": "3.10.0",
    "vite": "4.4.6",
    "vite-tsconfig-paths": "4.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.1.0",
    "@testing-library/react": "14.0.0",
    "@types/eslint": "8.44.2",
    "@types/node": "18.16.18",
    "@types/prettier": "2.7.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@vitest/coverage-v8": "0.33.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-array-func": "3.1.8",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-import-access": "2.1.1",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-prefer-arrow-functions": "3.1.4",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-rulesdir": "0.2.2",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-toplevel": "1.0.1",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-vitest": "0.2.8",
    "happy-dom": "10.5.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.3",
    "tsd": "0.28.1",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  }
}
